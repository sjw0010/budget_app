<h3>Calendar Info</h3>
      <%= f.label :start_date %>
      <div class="well">
  <div id="datetimepicker" class="input-append">
  	<%= f.text_field :start_date, 'data-format' => 'yyyy-MM-dd' %>
    <span class="add-on">
      <i data-time-icon="icon-time" data-date-icon="icon-calendar">
      </i>
    </span>
  </div>
</div>
<script type="text/javascript">
  $(function() {
    $('#datetimepicker').datetimepicker({
      pickTime: false
    });
  });
</script>